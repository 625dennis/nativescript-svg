<Page xmlns="http://schemas.nativescript.org/tns.xsd" 
  xmlns:svg="nativescript-svg" navigatingTo="onNavigatingTo">
  <StackLayout>
    <Label text="Tap the button" class="title"/>
    <Button text="TAP" tap="{{ onTap }}" />
    <Label text="{{ message }}" class="message" textWrap="true"/>
    <Label text="{{ message2 }}" class="message" textWrap="true"/>
    <svg:SVGImage src="~/images/nativescript.svg" height="100"/>
    <svg:SVGImage src="https://upload.wikimedia.org/wikipedia/commons/0/02/SVG_logo.svg" height="110"/>
    <Image src="https://en.wikipedia.org/static/images/project-logos/enwiki.png" height="100" /> 
  </StackLayout>
</Page>
